// Countdown
function updateCountdown(){var t=new Date('December 18, 2025 08:00:00').getTime(),n=new Date().getTime(),d=t-n;if(d<0)return;document.getElementById('days').textContent=Math.floor(d/86400000).toString().padStart(2,'0');document.getElementById('hours').textContent=Math.floor((d%86400000)/3600000).toString().padStart(2,'0');document.getElementById('minutes').textContent=Math.floor((d%3600000)/60000).toString().padStart(2,'0');document.getElementById('seconds').textContent=Math.floor((d%60000)/1000).toString().padStart(2,'0')}updateCountdown();setInterval(updateCountdown,1000);

// Hero Image Rotation
var currentImg=0,images=[document.getElementById('heroImg1'),document.getElementById('heroImg2')];function rotateImages(){images[currentImg].style.opacity='0';currentImg=(currentImg+1)%2;images[currentImg].style.opacity='1'}setInterval(rotateImages,2000);

// Hero Stats
function animateCount(el,start,end,suffix,duration,decimal){let startTime=null;const step=(timestamp)=>{if(!startTime)startTime=timestamp;const progress=Math.min((timestamp-startTime)/duration,1);const current=decimal?((progress*(end-start)+start).toFixed(1)):(Math.floor(progress*(end-start)+start));if(current>=1000){el.textContent=Math.floor(current/1000).toLocaleString()+","+(current%1000).toString().padStart(3,"0").slice(0,-2)+"00+"}else{el.textContent=current+suffix}if(progress<1)requestAnimationFrame(step)};requestAnimationFrame(step)}
setTimeout(()=>{animateCount(document.getElementById("countStudents"),0,10000,"+",2000,false);animateCount(document.getElementById("countPlaced"),0,97,"%",2000,false);animateCount(document.getElementById("countRating"),0,4.9,"/5",2000,true)},500);

// Big Count Animation
function animateBigCount(){var el=document.getElementById('bigStudentCount');var start=0;var end=10000;var duration=2500;var startTime=null;function step(timestamp){if(!startTime)startTime=timestamp;var progress=Math.min((timestamp-startTime)/duration,1);var easeOut=1-Math.pow(1-progress,3);var current=Math.floor(easeOut*(end-start)+start);if(current>=1000){el.textContent=Math.floor(current/1000).toLocaleString()+','+(current%1000).toString().padStart(3,'0')+'+'}else{el.textContent=current+'+'}if(progress<1){requestAnimationFrame(step)}else{el.textContent='10,000+'}}requestAnimationFrame(step)}

function animatePlacement(){var el=document.getElementById('placementRate');if(!el)return;var start=0;var end=97;var duration=2000;var startTime=null;function step(timestamp){if(!startTime)startTime=timestamp;var progress=Math.min((timestamp-startTime)/duration,1);var easeOut=1-Math.pow(1-progress,3);var current=Math.floor(easeOut*(end-start)+start);el.textContent=current+'%';if(progress<1)requestAnimationFrame(step)}requestAnimationFrame(step)}

var testimonialSection=document.querySelector('#bigStudentCount');var hasAnimated=false;function checkVisibility(){var rect=testimonialSection.getBoundingClientRect();var isVisible=rect.top<window.innerHeight&&rect.bottom>0;if(isVisible&&!hasAnimated){hasAnimated=true;animateBigCount()}}
window.addEventListener('scroll',checkVisibility);window.addEventListener('load',function(){setTimeout(checkVisibility,100)});

// Testimonial Slider
var currentSlide=0;var totalSlides=4;var slides=document.querySelectorAll('.testimonial-slide');var autoSlideInterval;
function showSlide(index){slides.forEach(function(slide,i){slide.classList.remove('active');if(i===index){slide.classList.add('active')}})}
function nextSlide(){currentSlide=(currentSlide+1)%totalSlides;showSlide(currentSlide);resetAutoSlide()}
function prevSlide(){currentSlide=(currentSlide-1+totalSlides)%totalSlides;showSlide(currentSlide);resetAutoSlide()}
function resetAutoSlide(){clearInterval(autoSlideInterval);autoSlideInterval=setInterval(nextSlide,4000)}
autoSlideInterval=setInterval(nextSlide,4000);

// Quote Slider
var currentQuote=0;var totalQuotes=4;var quoteSlides=document.querySelectorAll('.quote-slide');
function showQuote(index){quoteSlides.forEach(function(slide,i){slide.classList.remove('active');if(i===index){slide.classList.add('active')}})}
function nextQuote(){currentQuote=(currentQuote+1)%totalQuotes;showQuote(currentQuote)}
setInterval(nextQuote,3000);

// STATS PHOTOS SLIDER - 4 rotating images
var currentStatsImg = 0;
var totalStatsImgs = 4;
var statsImgs = [
  document.getElementById('statsImg1'),
  document.getElementById('statsImg2'),
  document.getElementById('statsImg3'),
  document.getElementById('statsImg4')
];

function rotateStatsImgs() {
  statsImgs[currentStatsImg].classList.remove('active');
  currentStatsImg = (currentStatsImg + 1) % totalStatsImgs;
  statsImgs[currentStatsImg].classList.add('active');
}

// Rotate every 3 seconds
setInterval(rotateStatsImgs, 3000);
</script>
