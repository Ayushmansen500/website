<style>
p:empty { display: none; }
* { box-sizing: border-box; }
body { margin:0; padding:0; font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif; }
.step-label { flex-shrink: 0; font-size:26px; font-weight:bold; color:#333; display:flex; align-items:center; margin-right:25px; }
.step-label span { font-size:30px; margin-left:8px; }
.embed-responsive { position:relative; width:100%; padding-bottom:56.25%; height:0; }
.embed-responsive iframe { position:absolute; top:0; left:0; width:100%; height:100%; display:block; }
.playPauseBtn { background: rgba(0,0,0,0.7); color:white; border:none; padding:8px 15px; border-radius:5px; cursor:pointer; font-size:14px; }
.playPauseBtn:hover { background: rgba(0,0,0,0.9); }
#formPopup { max-width:90%; width:350px; }
@media(max-width:768px){ .row.clearfix{flex-direction:column; align-items:center; text-align:center; width:100%;} .step-label{margin-right:0;margin-bottom:10px;font-size:20px;} .step-label span{font-size:24px;} .column.full{width:100%;} .embed-responsive{padding-bottom:56.25%; height:auto;} .embed-responsive iframe{position:absolute; width:100%; height:100%;} .playPauseBtn{padding:6px 12px; font-size:12px; bottom:10px !important; left:10px !important;} .row.clearfix p{font-size:14px !important;} h2{font-size:22px !important;} p{font-size:16px !important;} }
</style><script src="https://player.vimeo.com/api/player.js"></script><script>
window.addEventListener('load', function() {
    const iframe1 = document.getElementById("video1");
    const iframe2 = document.getElementById("video2");
    const iframe3 = document.getElementById("video3");
    const formPopup = document.getElementById("formPopup");
    const unmuteForm = document.getElementById("unmuteForm");
    const playPause1 = document.getElementById("playPause1");
    const playPause2 = document.getElementById("playPause2");
    const playPause3 = document.getElementById("playPause3");
    const unmuteOverlay = document.getElementById("unmuteOverlay");
    const afterVideo3Btns = document.getElementById("afterVideo3Btns");

    let player1, player2, player3;
    let userInteracted = false, formShown = false, video2Unlocked = false, video3Unlocked = false;

    function disableAllPlayPauseButtons(){ 
        playPause1.disabled = true; 
        playPause2.disabled = true; 
        playPause3.disabled = true; 
    }
    function enablePlayPauseButton(button){ if(button) button.disabled = false; }

    function initPlayers(){
        try {
            if(typeof Vimeo !== 'undefined'){
                player1 = new Vimeo.Player(iframe1);
                player2 = new Vimeo.Player(iframe2);
                player3 = new Vimeo.Player(iframe3);
                setupEvents();
            } else { setTimeout(initPlayers, 1000); }
        } catch(e){ setTimeout(initPlayers, 2000); }
    }

    function setupEvents(){
        playPause1.addEventListener('click', ()=>{ player1.getPaused().then(p=>p ? player1.play() : player1.pause()); });
        playPause2.addEventListener('click', ()=>{ if(!video2Unlocked) return; player2.getPaused().then(p=>p ? player2.play() : player2.pause()); });
        playPause3.addEventListener('click', ()=>{ if(!video3Unlocked) return; player3.getPaused().then(p=>p ? player3.play() : player3.pause()); });

        player1.on('seeked', ()=>{ player1.getCurrentTime().then(t=>{ if(!userInteracted && t > 11) player1.setCurrentTime(10); }); });
        player1.on('play', ()=>{ playPause1.style.display='inline-block'; if(!formShown) enablePlayPauseButton(playPause1); playPause1.textContent='‚è∏Ô∏è Pause'; });
        player1.on('pause', ()=>{ playPause1.textContent='‚ñ∂Ô∏è Play'; });
        player1.on('ended', ()=>{ unlockVideo2(); });

        player2.on('seeked', data=>{ player2.getCurrentTime().then(t=>{ if(t > data.seconds + 2) player2.setCurrentTime(data.seconds); }); });
        player2.on('play', ()=>{ if(video2Unlocked){ playPause2.style.display='inline-block'; enablePlayPauseButton(playPause2); playPause2.textContent='‚è∏Ô∏è Pause'; }});
        player2.on('pause', ()=>{ playPause2.textContent='‚ñ∂Ô∏è Play'; });
        player2.on('ended', ()=>{ unlockVideo3(); });

        player3.on('seeked', data=>{ player3.getCurrentTime().then(t=>{ if(t > data.seconds + 2) player3.setCurrentTime(data.seconds); }); });
        player3.on('play', ()=>{ if(video3Unlocked){ playPause3.style.display='inline-block'; enablePlayPauseButton(playPause3); playPause3.textContent='‚è∏Ô∏è Pause'; }});
        player3.on('pause', ()=>{ playPause3.textContent='‚ñ∂Ô∏è Play'; });
        player3.on('ended', ()=>{
            if(afterVideo3Btns){
                afterVideo3Btns.style.filter="none";
                afterVideo3Btns.style.pointerEvents="auto";
                afterVideo3Btns.style.opacity="1";
            }
        });
    }

    function showForm(){ 
        if(!formShown){
            formShown = true;
            formPopup.style.display = "block";
            disableAllPlayPauseButtons();
            player1.pause();
            unmuteOverlay.style.display = "none";
        } 
    }

    function unlockVideo2(){
        video2Unlocked = true;
        iframe2.style.pointerEvents = 'auto';
        iframe2.style.filter = 'none';
        const msg2 = iframe2.parentElement.parentElement.querySelector('p');
        if(msg2) msg2.style.display = 'none';

        // Force volume before playing (important for desktop autoplay policies)
        player2.setMuted(false).then(() => player2.setVolume(1)).then(() => {
            return player2.play();
        }).catch(err => {
            console.warn("Autoplay with sound blocked for video 2", err);
        });

        playPause2.style.filter="none";
        playPause2.style.pointerEvents="auto";
        playPause2.disabled=false;
    }

    function unlockVideo3(){
        video3Unlocked = true;
        iframe3.style.pointerEvents = 'auto';
        iframe3.style.filter = 'none';
        const msg3 = iframe3.parentElement.parentElement.querySelector('p');
        if(msg3) msg3.style.display = 'none';

        player3.setMuted(false).then(() => player3.setVolume(1)).then(() => {
            return player3.play();
        }).catch(err => {
            console.warn("Autoplay with sound blocked for video 3", err);
        });

        playPause3.style.filter="none";
        playPause3.style.pointerEvents="auto";
        playPause3.disabled=false;
    }

    document.body.addEventListener('click', ()=>{ userInteracted = true; }, {once:true});

    unmuteForm.addEventListener('submit', async e=>{
        e.preventDefault();
        const submitBtn = unmuteForm.querySelector('button[type="submit"]');
        submitBtn.disabled = true; 
        submitBtn.textContent = 'Submitting...';
        try{
            await new Promise(r=>setTimeout(r,1500)); // fake delay
            formPopup.style.display = "none";
            userInteracted = true;
            player1.setMuted(false).then(() => player1.setVolume(1)).then(() => {
                return player1.play();
            }).catch(err => console.warn('Autoplay with sound failed', err));
            playPause1.disabled = false;
            submitBtn.textContent = 'Keep Watching';
            submitBtn.disabled = false;
        } catch(error){
            alert('Submission failed. Please try again.');
            submitBtn.disabled = false;
            submitBtn.textContent = 'Keep Watching';
        }
    });

    unmuteOverlay.addEventListener("click", ()=>{ showForm(); });

    if(afterVideo3Btns){
        afterVideo3Btns.style.filter = "blur(4px)";
        afterVideo3Btns.style.pointerEvents = "none";
        afterVideo3Btns.style.opacity = "0.6";
    }

    setTimeout(initPlayers, 500);
});
</script><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?family=Architects Daughter" rel="stylesheet" property="stylesheet" type="text/css"><link href="//fonts.googleapis.com/css?